<table class="tablemap" background ="<%= asset_path('Mapemon.png')%>" id="screen" >
	<% 10.times do |i|%>
		<tr>
			<% 10.times do |j| %>
				<% if i == $game.player_x and j == $game.player_y %>
					<td class="tdmap" background ="<%= asset_path('player.png')%>" ></td>
				<% else %>
					<td class="tdmap"></td>
				<%end%>
			<%end%>
		</tr>
	<% end %>
</table>
<map name="control" id="control">
	<area alt="Top" title="top" href="/up" shape="rect" coords="79,500,125,539" />
	<area alt="Left" title="left" href="/left" shape="rect" coords="39,539,80,585" />
	<area alt="Bottom" title="bottom" href="/down" shape="rect" coords="81,586,125,625" />
